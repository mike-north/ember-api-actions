  "dependencies": {
    "ember-cli-babel": "^7.1.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mike.works/ember-api-actions"
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
