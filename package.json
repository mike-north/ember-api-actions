  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "@ember/test-helpers": "^1.0.0",
    "@types/ember": "^3.0.19",
    "@types/ember-data": "^2.14.18",
    "@types/ember-qunit": "^3.0.3",
    "@types/ember__test-helpers": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.3.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "0.2.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-pretender": "^3.0.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "^2.0.0",
    "ember-data": "~3.4.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^1.0.0",
