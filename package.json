{
  "name": "ember-api-actions",
  "version": "0.0.0-development",
  "description": "Trigger remote actions that don't fit into CRUD, in your Ember.js apps",
  "keywords": [
    "ember-addon",
    "api",
    "data",
    "request",
    "ajax",
    "model"
  ],
  "homepage": "https://github.com/mike-north/ember-api-actions",
  "repository": "https://github.com/mike-north/ember-api-actions.git",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:ts": "tslint --project .",
    "lint:hbs": "ember-template-lint .",
    "start": "ember server",
    "test": "ember test",
    "semantic-release": "semantic-release",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.3",
    "ember-cli-typescript": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.6",
    "@commitlint/config-conventional": "8.3.6",
    "@commitlint/travis-cli": "8.3.6",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.5.0",
    "@mike-north/js-lib-renovate-config": "1.3.1",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "@types/ember": "3.16.5",
    "@types/ember-data": "3.16.15",
    "@types/ember-qunit": "3.4.15",
    "@types/ember__test-helpers": "1.7.5",
    "@types/qunit": "2.11.3",
    "@types/rsvp": "4.0.4",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "4.0.1",
    "ember-cli-code-coverage": "1.0.3",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-htmlbars": "6.0.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-pretender": "4.0.0",
    "ember-cli-test-loader": "3.0.0",
    "ember-cli-tslint": "0.1.4",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-code-snippet": "2.4.1",
    "ember-data": "3.27.1",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "4.0.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "1.14.0",
    "ember-try": "1.4.0",
    "husky": "6.0.0",
    "json-typescript": "1.1.1",
    "jsonapi-typescript": "0.1.3",
    "loader.js": "4.7.0",
    "qunit": "2.16.0",
    "qunit-dom": "2.0.0",
    "semantic-release": "17.4.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://mike.works/ember-api-actions"
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  },
  "volta": {
    "node": "12.22.2",
    "yarn": "1.22.10"
  }
